[
    {
      "name": "ms-crm-queue",
      "image": "904761875964.dkr.ecr.ap-southeast-2.amazonaws.com/services/crm:latest",
      "cpu": 512,
      "memory": 512,
      "essential": true,
      "portMappings": [
            {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
            }
        ],
        "entryPoint": ["php"],
        "command": ["/var/www/html/bin/scheduler.php"],
        "logConfiguration": 
        {
            "logDriver": "awslogs",
            "options": 
            {
                "awslogs-group": "/ecs/services/crm",
                "awslogs-region": "ap-southeast-2",
                "awslogs-stream-prefix": "ms-crm-queue"
            }
        }
    }
]